    body{
      font-family: 'Montserrat', sans-serif;
      padding-bottom: 30vh;
    }

    .container {
      width: 100vw;
      max-width: 1251px;
      margin: 0 auto;
      /* border: 1px solid red; */
      display: flex;
    }

    .content {
      width: 70%;
      max-width: 933px;
      /* border: 1px solid red; */
      box-sizing: border-box;
    }

    .sticky-nav {
      width: 30%;
      box-sizing: border-box;
      order: 1;
    }

    h2{
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 20px;
      line-height: 24px;

      /* Green 1 */
      color: #219653;
      margin-top: 45px;
      margin-bottom: 22px;
    }

    .disc-block {
      display: grid;
      grid-template-rows: 1fr 1fr 1fr;
      grid-template-columns: 1fr 1fr;
      /* Gray 6 */
      background: #F2F2F2;
      /* Gray 6 */
      border: 1px solid #F2F2F2;
      box-sizing: border-box;
      padding: 14px 34px;

      font-family: Montserrat;
      font-style: normal;
      font-size: 15px;
      line-height: 27px;

      /* Gray 1 */
      color: #333333;

    }

    .disc-block>div:nth-last-child(2) {
      grid-column: 2/3;
      grid-row: 1/3;
    }

    .disc-block>div:nth-last-child(2) div {
      border: 1px dashed darkseagreen;
      border-radius: 5px;
    }

    .disc-block>div:nth-last-child(1) {
      grid-column: 2/3;
      grid-row: 3/4;
    }



    .disc-block>div {
      display: grid;
      grid-template-columns: 27px 1fr;
      grid-template-rows: auto;
    }

    .disc-block>div svg {
      grid-column: 1/2;
    }

    .disc-block>div div{
      grid-column: 2/3;
      padding: 0 10px;
      margin-top: -1px;
    }

    .disc-block > * {
      margin: 10px 0;
    }

    .para-h {
      font-weight: bold;
    }

    .para-d {
      font-weight: normal;
    }

    .title-bar {
      display: flex;
      align-items: center;
      justify-content: space-between;
      /* Gray 6 */
      color: #F2F2F2;
      font-weight: bold;
      /* Green 1 */
      background: #219653;
      cursor: pointer;
    }

    .title-bar > div {
      display: flex;
      align-items: center;
    }

    .title-bar > div > p, .lower-bar > p {
      display: inline-block;
      padding-left: 11px;
      padding-right: 11px;
      line-height: 22px;
    }

    .title-bar, .lower-bar {
      padding: 0 34px;
      min-height: 56px;
    }

    .lower-bar {
      display: flex;
      align-items: center;
      background: #F2F2F2;
      border: 1px solid #E0E0E0;
      font-size: 15px;
      box-sizing: border-box;
    }

    .lower-bar > *:nth-last-child(1) {
      margin-left: auto;
    }

    .lower-bar.topic, .lower-bar.important {
      align-items: start;
      padding-top: 17px;
      padding-bottom: 14px;
    }

    .lower-bar.video {
      /* font-weight: bold; */
      /* opacity: 0.8; */
    }

    .video > p:nth-last-child(1) {
      padding: 2px 11px;
      border: 1px solid #219653;
      background: transparent;
      color: #219653;
      font-size: 15px;
      font-weight: bold;
    }

    .subj-card {
      width: 100%;
      max-width: 243px;
      height: auto;
      background: #F2F2F2;
      color: #219653;
      opacity: 0.8;

      margin-bottom: 20px;
      border-radius: 2px;
      padding: 16px;
      box-sizing: border-box;
      border: 1px solid transparent;
    }

    .subj-card:hover {
      opacity: 1;
      cursor: pointer;
      border:1px dashed #219653;
    }

    .subj-card.active {
      background: #219653;
      color: #F2F2F2;
      opacity: 1;
    }

    .subj-card > p:nth-child(1) {
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 33px;
      line-height: 178.4%;
    }

    .subj-card > p:nth-child(2) {
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 15px;
      line-height: 27px;
    }

    .sticky-nav > * {
      max-width: 243px;
      margin-right: auto;
      margin-left: auto;
    }

    .sticky-nav > *:not(:nth-child(1)) {
      margin-bottom: 10px;
    }

    .partition {
      height: 10px;
    }

    .topics_list {
      line-height: 30px;
      padding-left: 11px;
      margin-top: -3px;
      width: fit-content;
      margin-left: 0 !important;
    }

    .topics_list > *:not(:nth-child(1)) {
      display: inline-flex;
      background: yellowgreen;
      padding: 0px 12px;
      margin: 2px;
      color: black;
      font-weight: bold;
      font-size: 12px;
    }

    .topics_list > *:nth-child(1) {
      display: block;
      background: none;
      color: black;
      padding: 0;
      margin-bottom: 6px;
      font-weight: normal;
    }

    .imp_list, .questions_list {
      line-height: 30px;
      padding-left: 9px;
      margin-top: -3px;
      width: fit-content;
      margin-left: 0 !important;
    }

    .imp_list > *:not(:nth-child(1)) {
      display: inline-flex;
      background: lightcoral;
      padding: 0px 12px;
      margin: 2px;
      color: white;
      font-weight: bold;
      font-size: 12px;
    }

    .imp_list > *:nth-child(1), .questions_list > *:nth-child(1) {
      display: block;
      background: none;
      color: black;
      padding: 0;
      padding-left: 3px;
      margin-bottom: 6px;
      font-weight: normal;
    }

    .questions_list > *:not(:nth-child(1)) {
      align-content: normal;
      display: inline-flex;
      padding: 2px 12px;
      margin: 3px;
      color: white;
      font-weight: normal;
      font-size: 15px;
      background: #219653;
      width: fit-content;
      display: block;
    }

    button, a {
      padding: 2px 11px;
      border: 1px solid #219653;
      background: transparent;
      color: #219653;
      font-size: 15px;
      font-weight: bold;
      text-decoration: none;
      font-family: system-ui;
      white-space: nowrap;
    }

    button:hover, a:hover {
      background: #219653;
      cursor: pointer;
      color: white;
    }

    .overlay {
      height: 100vh;
      width: 100vw;
      position: fixed;
      display: none;
    }

    .overlay.show {
      display: block;
      background: black;
      z-index: 10;
    }

    .overlay > iframe {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%,-50%);
    }

    .overlay > button {
      position: absolute;
      top: 1rem;
      left: 1rem;
      z-index: 100;
    }

    .overlay > h6 {
      position: absolute;
      top: 1.3rem;
      left: 6rem;
      z-index: 100;
      color: #219653;
      font-weight: bold;
    }

    .overlay > img {
      object-fit: contain;
      width: 100%;
      max-width: 1080px;
      max-height: 90vh;
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%,-50%);
    }

    .overlay > p {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%);
      color: #219653;
      max-width: 600px;
      width: 100vw;
    }

    .overlay > svg {
      position: absolute;
      top: 50%;
      cursor: pointer;
      opacity: 0.5;
      background: black;
    }

    .overlay > svg:nth-last-child(1) {
      left: 1rem;
    }

    .overlay > svg:nth-last-child(2) {
      right: 1rem;
    }

    .title-bar.not-collapsed .closed {
      display: none;
    }

    .title-bar:not(.not-collapsed) .opened {
      display: none;
    }

    .collapse_all {
      margin-left: auto;
      position: absolute;
      right: 0;
      top: -46px;
      margin-right: 34px;
    }

    .content-block svg {
      min-width: fit-content;
      width: auto;
    }

    .d-none {
      display: none;
    }

    .error {
      text-align: center;
      font-weight: bold;
      color: red;
    }

    .section {
      background: lightcoral !important;
    }

    @media (max-width: 700px) {

      h2 {
        margin-top: 10px;
      }

      .container {
        flex-wrap: wrap;
      }

      .sticky-nav {
        width: 100%;
        order: 0;
        padding: 0 10px;
      }

      .sticky-nav > * {
        max-width: 100%;
      }

      .content {
        padding: 0 10px;
        width: 100%;
      }

      .disc-block {
        grid-template-columns: 1fr;
        grid-template-rows: auto;
        padding: 0px;
        width: 100%;
      }

      .disc-block>div:nth-last-child(2), .disc-block>div:nth-last-child(1) {
        grid-column: auto;
        grid-row: auto;
      }

      .title-bar, .lower-bar {
        padding: 0px 2px;
      }

      .collapse_all {
        margin-right: 0px;
      }

      .overlay>p {
        text-align: center;
      }

      .subj-card > p:nth-child(1) {
        display: none;
      }

      .title-bar > div > p, .lower-bar > p {
        padding-left: 4px;
        padding-right: 4px;
      }
    }
    </style>
  </head>
  <body>
    <div class="overlay">
      <h6></h6>
      <img src="" alt="">
      <iframe src="" width="100%" height="100%"></iframe>
      <p></p>
      <button type="button" name="button">Close</button>
      <svg class="next" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6z" fill="white"/><path d="M0 0h24v24H0z" fill="none"/></svg>
      <svg class="prev" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M15.41 7.41L14 6l-6 6 6 6 1.41-1.41L10.83 12z" fill="white"/><path d="M0 0h24v24H0z" fill="none"/></svg>
    </div>
    <div class="container">

      <div class="sticky-nav">
        <h2>Subjects</h2>
        <div class="subj-card {{sre}}" onClick="window.location = '/?pagerequest=sre';">
          <p>01</p>
          <p>Software Requirements Engineering</p>
        </div>

        <div class="subj-card {{tpl}}" onClick="window.location = '/?pagerequest=tpl';">
          <p>02</p>
          <p>Theory of Programming Languages</p>
        </div>

        <div class="subj-card {{algo}}" onClick="window.location = '/?pagerequest=algo';">
          <p>03</p>
          <p>Advanced Theory and Design of Algorithums</p>
        </div>

        <div class="subj-card {{is}}" onClick="window.location = '/?pagerequest=is';">
          <p>04</p>
          <p>Information Security</p>
        </div>

      </div>

      <div class="content">
        <h2>Subject Details</h2>
        <div class="disc-block">
          <div class="">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
            <g opacity="0.54">
            <path d="M18 2H6C4.9 2 4 2.9 4 4V20C4 21.1 4.9 22 6 22H18C19.1 22 20 21.1 20 20V4C20 2.9 19.1 2 18 2ZM6 4H11V12L8.5 10.5L6 12V4Z" fill="black"/>
            </g>
            </svg>
            <div class="">
              <p class="para-h">Subject</p>
              <p class="para-d">{{sorted.[0].Subject}}</p>
            </div>
          </div>
          <div class="">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
            <g opacity="0.54">
            <path d="M12 5.9C13.16 5.9 14.1 6.84 14.1 8C14.1 9.16 13.16 10.1 12 10.1C10.84 10.1 9.9 9.16 9.9 8C9.9 6.84 10.84 5.9 12 5.9ZM12 14.9C14.97 14.9 18.1 16.36 18.1 17V18.1H5.9V17C5.9 16.36 9.03 14.9 12 14.9ZM12 4C9.79 4 8 5.79 8 8C8 10.21 9.79 12 12 12C14.21 12 16 10.21 16 8C16 5.79 14.21 4 12 4ZM12 13C9.33 13 4 14.34 4 17V20H20V17C20 14.34 14.67 13 12 13Z" fill="black"/>
            </g>
            </svg>
            <div class="">
              <p class="para-h">Instructor</p>
              <p class="para-d">{{sorted.[0].Instructor}}</p>
            </div>
          </div>
          <div class="">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
            <g opacity="0.54">
            <path d="M9 11.75C8.31 11.75 7.75 12.31 7.75 13C7.75 13.69 8.31 14.25 9 14.25C9.69 14.25 10.25 13.69 10.25 13C10.25 12.31 9.69 11.75 9 11.75ZM15 11.75C14.31 11.75 13.75 12.31 13.75 13C13.75 13.69 14.31 14.25 15 14.25C15.69 14.25 16.25 13.69 16.25 13C16.25 12.31 15.69 11.75 15 11.75ZM12 2C6.48 2 2 6.48 2 12C2 17.52 6.48 22 12 22C17.52 22 22 17.52 22 12C22 6.48 17.52 2 12 2ZM12 20C7.59 20 4 16.41 4 12C4 11.71 4.02 11.42 4.05 11.14C6.41 10.09 8.28 8.16 9.26 5.77C11.07 8.33 14.05 10 17.42 10C18.2 10 18.95 9.91 19.67 9.74C19.88 10.45 20 11.21 20 12C20 16.41 16.41 20 12 20Z" fill="black"/>
            </g>
            </svg>
            <div class="">
              <p class="para-h">Class Senior</p>
              <p class="para-d">{{sorted.[0].ClassSenior}}</p>
            </div>
          </div>
          <div class="">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
            <g opacity="0.54">
            <path d="M17 3H7C5.9 3 5.01 3.9 5.01 5L5 21L12 18L19 21V5C19 3.9 18.1 3 17 3Z" fill="black"/>
            </g>
            </svg>
            <div class="">
              <p class="para-h">Special Note</p>
              <p class="para-d">{{sorted.[0].Note}}</p>
            </div>
          </div>
          <div class="">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
            <g opacity="0.54">
            <path d="M11.99 2C6.47 2 2 6.48 2 12C2 17.52 6.47 22 11.99 22C17.52 22 22 17.52 22 12C22 6.48 17.52 2 11.99 2ZM12 20C7.58 20 4 16.42 4 12C4 7.58 7.58 4 12 4C16.42 4 20 7.58 20 12C20 16.42 16.42 20 12 20Z" fill="black"/>
