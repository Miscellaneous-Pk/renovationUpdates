  <script type="text/javascript">

    $(document).ready(() => {
      $('.title-bar:eq(-1)').trigger('click');
    })

    $('.video button, .quiz button, .book button').on('click',function() {
      $('.overlay').find('h6').html('');
      $('.overlay').find('svg').addClass('d-none');
      $('.overlay').addClass('show').find('p').html('');
      $('.overlay').addClass('show').find('iframe').attr({src:''});
      $('.overlay').addClass('show').find('img').attr({src:''});
      if ($(this).attr('link')) {
        $('.overlay').find('h6').html(`${$(this).closest('div').find('p:eq(0)').html()}`);
        return $('.overlay').addClass('show').find('iframe').attr({src:$(this).attr('link')});
      }
      return $('.overlay').addClass('show').find('p').html($(this).attr('msg'));
    })

    $('.assignment button').on('click',function() {
      $('.overlay').addClass('show').find('p').html('');
      $('.overlay').find('svg').removeClass('d-none');
      $('.overlay').find('h6').html('');
      $('.overlay').addClass('show').find('iframe').attr({src:''});
      $('.overlay').addClass('show').find('img').attr({src:''});
      if ($(this).attr('img')) {
        let imgs = $(this).attr('img').split('*');
        console.log(imgs.length);
        $('.overlay').data({imgs,currentValue: 0});
        let currentimgtag = $('.overlay').find('img').attr({src: imgs[0]});
        $('.overlay').find('h6').html(`${$('.overlay').data().currentValue + 1} / ${$('.overlay').data().imgs.length} image(s)`);
        return $('.overlay').addClass('show')
      }
      return $('.overlay').addClass('show').find('p').html($(this).attr('msg'));
    })

    $('.next,.prev').on('click',function() {
      if ($(this).attr('class') == 'prev' && $('.overlay').data().currentValue > 0) $('.overlay').data().currentValue = $('.overlay').data().currentValue - 1;
      if ($(this).attr('class') == 'next' && $('.overlay').data().currentValue + 1 < $('.overlay').data().imgs.length) $('.overlay').data().currentValue = $('.overlay').data().currentValue + 1;

      let index = $('.overlay').data().currentValue;
      console.log({currentValue:$('.overlay').data().currentValue, imgs: $('.overlay').data().imgs,length:$('.overlay').data().imgs.length});
      $('.overlay').find('img').attr({src: $('.overlay').data().imgs[index]});
      $('.overlay').find('h6').html(`${$('.overlay').data().currentValue + 1} / ${$('.overlay').data().imgs.length} image(s)`);
    })

    $('.overlay>button').on('click',function() {
      console.log('hello');
      $(this).closest('.overlay').removeClass('show');
    })

    $('.collapse_all').on('click',function() {
      let collapsedCheck = $('.lower-bar.d-none').length > 0;
      if (collapsedCheck) {
        $('.lower-bar').removeClass('d-none');
        $('.title-bar').addClass('not-collapsed');
        return $(this).html('Collapse All');
      }
      $('.lower-bar').addClass('d-none');
      $('.title-bar').removeClass('not-collapsed');
      $(this).html('Show All');
    })

    $('.title-bar').on('click',function() {
      let target = $(this).attr('data-target');
      $(`[data-show=${target}]`).toggleClass('d-none');
      $(this).toggleClass('not-collapsed');
      if ($('.lower-bar.d-none').length > 0) $('.collapse_all').html('Show All');
      else $('.collapse_all').html('Collapse All');
    })
  </script>

